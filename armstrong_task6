#include<stdio.h>
main()
{
	printf("Enter number to be checked\n");int n=0;
	scanf("%d",&n);
	int i,j,k,l,m;j=0;i=n;int sum=0;l=1;int r=n;
	while(n!=0)
	{
		n=n/10;
		j++;
		}
		
		while(i!=0)
		{
		k=i%10;
		for(m=0;m<j;m++)
		l=l*k;
		sum=sum+l;
		k=0;
		i=i/10;l=1;
		}
		if(r==sum)
		printf("Armstrong");
		else
		printf("not armstrong");
}
